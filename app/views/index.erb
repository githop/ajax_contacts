<h1>Contacts list</h1>

<a href="/sessions/destroy">log out</a>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Phone</th>
      <th>Email</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @contacts.each do |contact| %>
      <%= erb :_contact_row, layout: false, locals: { contact: contact }  %>
    <% end %>
  </tbody>
</table>
<a href='/contacts/new'>add contact</a>




