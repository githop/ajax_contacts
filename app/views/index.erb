<h1>Contacts list</h1>

<a href="/sessions/destroy">log out</a>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Phone</th>
      <th>Email</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @contacts.each do |contact| %>
      <tr><a href="/contacts/<%=contact.id%>"><%=contact.id %></a></tr>
      <tr><%=contact.first_name %></tr>
      <tr><%=contact.last_name %></tr>
      <tr><%=contact.phone %></tr>
      <tr><%=contact.email %></tr>
      <tr>
	<a href="contacts/<%=contact.id%>/edit">Edit</a>
	<form action="/contacts/<%=contact.id%>/delete" method="post">
	  <input type="submit" value="delete">
	</form>
      </tr>
    <% end %>
  </tbody>
  <a href='/contacts/new'>add contact</a>
</table>





